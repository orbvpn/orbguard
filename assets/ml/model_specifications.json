  "models": [
    {
      "id": "scam_classifier",
      "name": "Scam/Phishing Text Classifier",
      "filename": "scam_classifier.tflite",
      "version": "1.0.0",
      "architecture": {
        "type": "text_classification",
        "framework": "tensorflow",
        "model_type": "bidirectional_lstm",
        "layers": [
          {"type": "embedding", "input_dim": 10000, "output_dim": 128},
          {"type": "bidirectional_lstm", "units": 64, "return_sequences": false},
          {"type": "dropout", "rate": 0.3},
          {"type": "dense", "units": 32, "activation": "relu"},
          {"type": "dropout", "rate": 0.2},
          {"type": "dense", "units": 2, "activation": "softmax"}
        ],
        "input_shape": [128],
        "output_shape": [2],
        "max_sequence_length": 128,
        "vocab_size": 10000
      },
      "training": {
        "dataset": "combined_scam_sms_dataset",
        "sources": [
          "SMS Spam Collection Dataset",
          "Kaggle Fraud SMS Dataset",
          "Custom curated phishing samples"
        ],
        "preprocessing": [
          "lowercase",
          "remove_special_chars",
          "tokenize",
          "pad_sequences"
        ],
        "augmentation": [
          "synonym_replacement",
          "random_deletion",
          "random_swap"
        ],
        "train_test_split": 0.8,
        "epochs": 50,
        "batch_size": 32,
        "optimizer": "adam",
        "learning_rate": 0.001,
        "loss": "categorical_crossentropy",
        "metrics": ["accuracy", "precision", "recall", "f1"]
      },
      "output_classes": ["legitimate", "scam"],
      "quantization": "float16",
      "target_size_mb": 5,
      "min_accuracy": 0.92,
      "fallback": "heuristic_patterns"
    },
    {
      "id": "url_classifier",
