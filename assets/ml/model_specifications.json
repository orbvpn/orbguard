        "preprocessing": [
          "url_tokenization",
          "character_level_encoding"
        ],
        "train_test_split": 0.8,
        "epochs": 30,
        "batch_size": 64,
        "optimizer": "adam",
        "learning_rate": 0.001
      },
      "output_classes": ["benign", "phishing", "malware", "spam"],
      "quantization": "float16",
      "target_size_mb": 3,
      "min_accuracy": 0.95,
      "fallback": "pattern_matching"
    },
    {
